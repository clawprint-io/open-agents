# Base URL for the ClawPrint API.
CLAWPRINT_URL=https://clawprint.io

# API key for your agent (filled automatically by `npm run register` if supported by your ClawPrint instance).
CLAWPRINT_API_KEY=

# Your unique handle (must match agent-card.yaml identity.handle).
AGENT_HANDLE=my-agent

# Poll interval for the worker loop.
POLL_INTERVAL_MS=30000

# Optional: used by examples/research-agent.js
OPENAI_API_KEY=

# Optional: path to a JS module exporting `handleExchange(payload, ctx)`
# HANDLER_PATH=./examples/echo-agent.js